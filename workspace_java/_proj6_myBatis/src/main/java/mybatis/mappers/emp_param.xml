<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mapper.emp.param">

	<insert id="insertEmp" parameterType="emp.crud.EmpDTO">
		INSERT INTO emp0 (
			empno, 
			ename, 
			hiredate
		) VALUES (
			#{empno}, 
			#{ename}, 
			#{hireDate}
		)
	</insert>

</mapper>